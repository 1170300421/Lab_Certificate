<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name></display-name>
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>CertificateServlet</servlet-name>
        <servlet-class>comWeb.CertificateServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>CertificateServlet</servlet-name>
        <url-pattern>/CertificateServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <description>This is the description of my LoginJ2EE component</description>
        <display-name>This is the display name of my LoginJ2EE component</display-name>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>comWeb.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <description>This is the description of my DigitalJ2EE component</description>
        <display-name>This is the display name of my DigitalJ2EE component</display-name>
        <servlet-name>DigitalServlet</servlet-name>
        <servlet-class>comWeb.DigitalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DigitalServlet</servlet-name>
        <url-pattern>/DigitalServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <description>This is the description of my ChangeJ2EE component</description>
        <display-name>This is the display name of my DigitalJ2EE component</display-name>
        <servlet-name>ChangeServlet</servlet-name>
        <servlet-class>comWeb.ChangeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangeServlet</servlet-name>
        <url-pattern>/ChangeServlet</url-pattern>
    </servlet-mapping>

</web-app>